extends ../layouts/common

block content
    h3 List users
            a(href="#" class="btn btn-success ml-2") Add user
            ul
                each user in users
                    li
                        span= user.name
                        a(href="/:id", class="btn btn-primary ml-2") View
                        a(href="#", class="btn btn-danger ml-2") Delete
                        a(href="#", class="btn btn-warning ml-2") Edit